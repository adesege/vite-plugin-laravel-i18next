{
  "name": "vite-plugin-laravel-i18next",
  "packageManager": "yarn@4.5.0",
  "version": "0.0.1",
  "description": "A Vite plugin to convert Laravel translation files to i18next-compatible JSON files",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test:unit": "vitest run --coverage",
    "build": "rm -rf dist && tsc",
    "prepublishOnly": "yarn build",
    "test:e2e": "playwright test",
    "dev:test": "vite --config tests/e2e/app/vite.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adesege/vite-plugin-laravel-i18next.git"
  },
  "keywords": [
    "vite",
    "plugin",
    "laravel",
    "i18next",
    "translation",
    "internationalization"
  ],
  "author": "Temitayo Fadojutimi <me@temitayo.dev>",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.47.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.1",
    "chokidar": "^4.0.1",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "i18next": "^23.15.1",
    "php-parser": "^3.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.2",
    "react-router-dom": "^6.26.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vitest": "^2.1.1"
  }
}
